<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           xmlns:i18n="http://namespaces.zope.org/i18n"
           i18n_domain="gs.profile.status.change">

  <i18n:registerTranslations directory="locales" />

  <!--Email command-->
  <adapter
    name="summary"
    for="gs.group.type.support.interfaces.IGSSupportGroup"
    provides="gs.group.list.command.interfaces.IEmailCommand"
    factory=".command.StatusCommand" />

  <!--The toggle page-->
  <browser:page
    name="notification.html"
    for="Products.CustomUserFolder.interfaces.ICustomUser"
    class=".page.ChangeNotification"
    template="browser/templates/page.pt"
    permission="zope2.ManageProperties" />

</configure>
